		<true/>
		<key>NSCameraUsageDescription</key>
		<string>Verdis Communications needs access to your camera for meetings.</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>Verdis Communications needs access to your microphone for meetings.</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>Verdis Communications requires the photo library to change profile pictures</string>
		<key>NSCalendarsUsageDescription</key>
		<string>Verdis Communications requires the calender to schedule events in meetings</string>
		<key>RTCAppGroupIdentifier</key>
		<string>group.com.garv.verdiscom</string>
		<key>RTCScreenSharingExtension</key>
		<string>com.garv.verdiscom.Broadcast-Extension</string>
