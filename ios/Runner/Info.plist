		<array>
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>com.googleusercontent.apps.686272364376-vtfumiv5sbu519jt58atmf8t0fkdir2n</string>
				</array>
			</dict>
		</array>
